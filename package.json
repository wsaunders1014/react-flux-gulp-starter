{
  "name": "react-gulp-starter",
  "version": "1.0.1",
  "engines": {
    "node": ">=5.0.0"
  },
  "description": "React-Gulp starter with common tasks and scenarios",
  "scripts": {
    "start": "npm run build:server && webpack-dashboard -c cyan -- node server-dev.js",
    "clean:build": "rimraf build",
    "build:html": "echo hi",
    "prestart": "npm run clean:build",
    "prebuild": "npm run clean:build",
    "build": "npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=production webpack --config ./config/webpack.config.production.js --progress",
    "build:server": "NODE_ENV=production webpack --config ./config/webpack.config.server.js --progress",
    "postbuild": "npm run build:html",
    "build:serve": "http-server build/client -p 3003 --cors -o",
    "lint": "eslint config src/js/** --ext .js",
    "jscs": "jscs src/js/",
    "typecheck": "flow",
    "test": "mocha --compilers js:babel-core/register,css:./test/unit/cssNullCompiler.js --require ./test/unit/testHelper.js --recursive ./test/unit",
    "test:watch": "npm run test -- --watch",
    "tunnel": "lt --port 3000"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MadeInHaus/react-gulp-starter.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extend": "^3.0.0",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "http-proxy": "^1.16.2",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
    "raw-loader": "^0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "^4.1.1",
    "sass-variable-loader": "0.0.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^2.1.0-beta.21",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-merge": "^2.4.0",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "classnames": "^2.2.5",
    "compression": "1.6.2",
    "connect-inject": "0.4.0",
    "debug": "^2.6.0",
    "dotenv": "^4.0.0",
    "express": "4.14.0",
    "express-state": "1.4.0",
    "fluxible": "^1.1.0",
    "fluxible-action-utils": "^0.2.4",
    "fluxible-addons-react": "^0.2.9",
    "fluxible-plugin-fetchr": "^0.3.11",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "2.7.0"
  }
}
